DATA tBLA g:1
DATA tBLA clr:1

// Signals using OSM or fast 5-PTs path
DATA tOSM q0
DATA tOSM q1
DATA tOSM q2
DATA tOSM q3
DATA tOSM q4
DATA tOSM q5
DATA tOSM q6
DATA tOSM q7
