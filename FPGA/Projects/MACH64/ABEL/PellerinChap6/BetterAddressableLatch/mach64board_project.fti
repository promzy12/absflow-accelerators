DATA tBSR q2.AR
DATA tBSR q3.AR
DATA tBSR q4.AR
DATA tBSR q5.AR
DATA tBSR q6.AR
DATA tBSR q7.AR

// Block Load Adders
DATA tBLA s0:1
DATA tBLA s1:1
DATA tBLA s2:1
